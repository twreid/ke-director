    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch go",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/starter",
            "env": {},
            "args": [
              "-config.file=${workspaceFolder}/config.yml",
              "-debug"
            ]
        },
        {
            "name": "Launch vue.js",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/app"
        },
    ]
}
